        </div>
        {{/oauth}} --}}
        </div>
        
      </div>
      <div class="col-md-5 col-lg-5">
        <div class="code-column">
          <div class='block request_url'></div>
          <h5>URL</h5>
          <pre><div class="method">{{method}}</div> <span>{{schemes}}://{{host}}{{basePath}}{{path}}</span></pre>
          <h5>Headers</h5>
          <pre><span class="authorization-header"></span><span class="request-headers"></span></pre>
          {{!-- <div class="curl"></div> --}}
        </div>
        </div>
      </div>

      {{#if headers}}
      <div class="row">
        <div class="col-md-7 col-lg-7">
          <h4 data-sw-translate>Headers</h4>
          <table class="headers">
            <thead>
              <tr>
                <th style="width: 100px; max-width: 100px" data-sw-translate>Header</th>
                <th style="width: 310px; max-width: 310px" data-sw-translate>Description</th>
                <th style="width: 200px; max-width: 200px" data-sw-translate>Type</th>
                <th style="width: 320px; max-width: 320px" data-sw-translate>Other</th>
              </tr>
            </thead>
            <tbody>
              {{#each headers}}
              <tr>
                <td>{{@key}}</td>
                <td>{{this.description}}</td>
                <td>{{this.type}}</td>
                <td>{{this.other}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <div class="col-md-5 col-lg-5">
          <div class='block request_headers'></div>

        </div>
      </div>
      {{/if}}

      {{#if parameters}}
      <div class="row">
      <form accept-charset='UTF-8' class='sandbox'>
          <div class="col-lg-12 col-md-12">
            <h4 data-sw-translate>Parameters</h4>
          </div>
      </div>
      <div class="row">
        <div class="operation-params">
        </div>
      </div>
      {{/if}}

      {{#if type}}
      <div class="row">
        <div class="col-lg-12 col-md-12">
          <h4 data-sw-translate>Responses</h4>
        </div>
      </div>
      <div class="row response-row">
        <div class="col-md-7 col-lg-7">
          <span class="model-signature"></span>
        </div>
        <div class="col-md-5 col-lg-5">
          <div class="code-column">
            <div class="snippet">
              <h5>HTTP Status</h5>
              <pre><code><div>200 OK</div></code></pre>
              <h5>Response Body</h5>
              <pre><code><div class="operation-response"></div></code></pre>
            </div>
          </div>
        </div>
      </div>

      {{/if}}

      {{#if responseMessages}}
      <div class="row response-row">
        <div class="col-lg-12 col-md-12">
          <h4 data-sw-translate>Additional Response Codes</h4>
        </div>
      </div>
      <div class="operation-status"></div>


      {{/if}}

      {{#if isReadOnly}}
      {{else}}
