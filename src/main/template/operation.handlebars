      <div class="col-md-12 col-lg-12">
      <div class='heading'>
        <h2>
          {{{summary}}}
        </h2>
      </div>
      </div>
    </div>
    
    <div class="row">
      <div class="col-md-7 col-lg-7">
      <div class='content' id='{{parentId}}_{{nickname}}_content'>
        {{#if deprecated}}
            <h4><span data-sw-translate>Warning: Deprecated</span></h4>
        {{/if}}
        {{#if description}}
        <div class="markdown">{{{description}}}</div>
        {{/if}}
{{!--         {{#oauth}}
